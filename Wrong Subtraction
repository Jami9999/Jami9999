#include <iostream>
#include <cstring>
#include <cmath>
using namespace std ;

int f (int n , int k) {
            for (int i = 0 ; i < k ; i ++) {
                        if (n % 10 == 0) {
                                    n /= 10 ;
                        } else {
                                    n -= 1 ;
                        }
            }
            cout << n << "\n" ;
            return n ;
}

int main () {
            int n , k ;
            cin >> n >> k ;
            f (n , k) ;
            return 0 ;
}
