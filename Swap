#include <iostream>
#include <cstring>
#include <cmath>
using namespace std ;

int f (int a , int b) {
            int temp ;
            temp = a ;
            a = b ;
            b = temp ;
            cout << "Number after swapping : " << a << " " << b << "\n" ;
            return 0 ;
}

int main () {
            int a , b ;
            cout << "Enter two numbers : " ;
            cin >> a >> b ;
            f (a , b) ;
            return 0 ;
}
