#include <iostream>
#include <cstring>
#include <cmath>
using namespace std ;

int f (int a , int b) {
            int y = 0 ;
            while (1) {
                        a *= 3 ;
                        b *= 2 ;
                        y ++ ;
                        if (a > b) {
                                    break ;
                        }
            }
            cout << y << "\n" ;
            return y ;
}

int main () {
            int a , b ;
            cin >> a >> b ;
            f (a , b) ;
            return 0 ;
}
