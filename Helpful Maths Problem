#include <iostream>
#include <cstring>
#include <cmath>
using namespace std ;

int f (string s) {
            int sum = 0 ;
            for (int i = 0 ; i < s.length () ; i ++) {
                        if(isdigit(s [i])) {
                                    sum += s [i] - '0' ;    
                        }
            }
            cout << sum << "\n" ;
            return sum ;
}

int main () {
            string s ;
            cin >> s ;
            f (s) ;
            return 0 ;
}
